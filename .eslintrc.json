{
  "extends": "google",
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": [2, "never"],
    "eol-last": [1, "always"],
    "linebreak-style": 0,
    "object-curly-spacing": [2, "always"],
    "semi": 2,
    "quotes": 0,
    "max-len": ["error", {"code": 100}]

  }
}
